  "keywords": [
    "typo3",
    "typo3 cms",
    "news",
    "newsslider"
  ],
  "homepage": "https://t3sbootstrap.de",
  "authors": [
    {
      "name": "Helmut Hackbarth",
      "role": "Developer",
